{
    "name": "waad/laravel-model-metadata",
    "description": "A Laravel package for handling metadata with JSON casting and custom relation names.",
    "type": "library",
    "authors": [
        {
            "name": "Waad Mawlood",
            "email": "waad_mawlood@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "laravel/framework": "^9.30.1|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest-plugin-laravel": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Waad\\Metadata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waad\\Metadata\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waad\\Metadata\\Providers\\MetadataServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test:coverage": "vendor/bin/pest --coverage",
        "lint": "vendor/bin/pint"
    },
    "keywords": [
        "laravel",
        "metadata",
        "model",
        "json",
        "eloquent"
    ],
    "license": "MIT"
}
